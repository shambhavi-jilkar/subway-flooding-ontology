@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix subway: <http://subway-emergency.org/ontology#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .

# Ontology Declaration
<http://subway-emergency.org/ontology> rdf:type owl:Ontology ;
    rdfs:label "Subway Emergency Communication Ontology" ;
    rdfs:comment "Dynamic ontology for enhanced emergency communication" .

# Top-Level Classes (aligned with BFO)
subway:Entity rdf:type owl:Class ;
    rdfs:label "Subway System Entity" ;
    rdfs:subClassOf bfo:BFO_0000001 .  # BFO Entity

# Infrastructure Classes
subway:Infrastructure rdf:type owl:Class ;
    rdfs:subClassOf subway:Entity ;
    rdfs:label "Infrastructure Component" .

subway:PowerSystem rdf:type owl:Class ;
    rdfs:subClassOf subway:Infrastructure ;
    rdfs:label "Power System" .

subway:SignalSystem rdf:type owl:Class ;
    rdfs:subClassOf subway:Infrastructure ;
    rdfs:label "Signal System" .

subway:DrainageSystem rdf:type owl:Class ;
    rdfs:subClassOf subway:Infrastructure ;
    rdfs:label "Drainage System" .

subway:TransitStructure rdf:type owl:Class ;
    rdfs:subClassOf subway:Infrastructure ;
    rdfs:label "Transit Structure" .

# Personnel Classes
subway:Personnel rdf:type owl:Class ;
    rdfs:subClassOf subway:Entity ;
    rdfs:label "Personnel" .

subway:FieldAgent rdf:type owl:Class ;
    rdfs:subClassOf subway:Personnel ;
    rdfs:label "Field Agent" .

subway:CommandPersonnel rdf:type owl:Class ;
    rdfs:subClassOf subway:Personnel ;
    rdfs:label "Command Personnel" .

# Government Classes
subway:Government rdf:type owl:Class ;
    rdfs:subClassOf subway:Entity ;
    rdfs:label "Government Entity" .

subway:TransitAuthority rdf:type owl:Class ;
    rdfs:subClassOf subway:Government ;
    rdfs:label "Transit Authority" .

subway:EmergencyAgency rdf:type owl:Class ;
    rdfs:subClassOf subway:Government ;
    rdfs:label "Emergency Agency" .

# Information Classes
subway:Information rdf:type owl:Class ;
    rdfs:subClassOf subway:Entity ;
    rdfs:label "Information Resource" .

subway:SensorData rdf:type owl:Class ;
    rdfs:subClassOf subway:Information ;
    rdfs:label "Sensor Data" .

subway:Protocol rdf:type owl:Class ;
    rdfs:subClassOf subway:Information ;
    rdfs:label "Emergency Protocol" .

# Environmental Classes
subway:Environmental rdf:type owl:Class ;
    rdfs:subClassOf subway:Entity ;
    rdfs:label "Environmental Factor" .

subway:Hazard rdf:type owl:Class ;
    rdfs:subClassOf subway:Environmental ;
    rdfs:label "Environmental Hazard" .

# Public Classes
subway:Public rdf:type owl:Class ;
    rdfs:subClassOf subway:Entity ;
    rdfs:label "Public Stakeholder" .

# Object Properties (Relationships)
subway:powers rdf:type owl:ObjectProperty ;
    rdfs:domain subway:PowerSystem ;
    rdfs:range subway:Infrastructure ;
    rdfs:label "powers" ;
    owl:inverseOf subway:poweredBy .

subway:controls rdf:type owl:ObjectProperty ;
    rdfs:domain subway:SignalSystem ;
    rdfs:range subway:TransitStructure ;
    rdfs:label "controls" ;
    owl:inverseOf subway:controlledBy .

subway:affects rdf:type owl:ObjectProperty, owl:TransitiveProperty ;
    rdfs:domain subway:Infrastructure ;
    rdfs:range subway:Infrastructure ;
    rdfs:label "affects" .

subway:dependsOn rdf:type owl:ObjectProperty ;
    rdfs:domain subway:Infrastructure ;
    rdfs:range subway:Infrastructure ;
    rdfs:label "depends on" .

subway:reportsTo rdf:type owl:ObjectProperty ;
    rdfs:domain subway:FieldAgent ;
    rdfs:range subway:CommandPersonnel ;
    rdfs:label "reports to" .

subway:monitors rdf:type owl:ObjectProperty ;
    rdfs:domain subway:SensorData ;
    rdfs:range subway:Infrastructure ;
    rdfs:label "monitors" .

subway:manages rdf:type owl:ObjectProperty ;
    rdfs:domain subway:TransitAuthority ;
    rdfs:range subway:Infrastructure ;
    rdfs:label "manages" .

# Data Properties (Attributes)
subway:connectionCount rdf:type owl:DatatypeProperty ;
    rdfs:domain subway:Entity ;
    rdfs:range xsd:integer ;
    rdfs:label "connection count" .

subway:criticalityScore rdf:type owl:DatatypeProperty ;
    rdfs:domain subway:Infrastructure ;
    rdfs:range xsd:float ;
    rdfs:label "criticality score" .

subway:dailyPassengerCount rdf:type owl:DatatypeProperty ;
    rdfs:domain subway:TransitStructure ;
    rdfs:range xsd:integer ;
    rdfs:label "daily passenger count" .

subway:operationalStatus rdf:type owl:DatatypeProperty ;
    rdfs:domain subway:Infrastructure ;
    rdfs:range xsd:string ;
    rdfs:label "operational status" .

subway:severityLevel rdf:type owl:DatatypeProperty ;
    rdfs:domain subway:Hazard ;
    rdfs:range xsd:float ;
    rdfs:label "severity level" .

subway:estimatedFailureTime rdf:type owl:DatatypeProperty ;
    rdfs:domain subway:Infrastructure ;
    rdfs:range xsd:duration ;
    rdfs:label "estimated failure time" .

